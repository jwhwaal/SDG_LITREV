   Juana Maria and Escrig-Olmedo, Elena and Fernandez-Izquierdo, Maria
   Angeles},
Title = {SDG reporting: an analysis of corporate sustainability leaders},
Journal = {MARKETING INTELLIGENCE \& PLANNING},
Year = {2023},
Volume = {41},
