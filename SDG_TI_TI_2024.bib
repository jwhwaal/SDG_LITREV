@article{ WOS:000558892800001,
Author = {Matteucci, Victoria},
Title = {How can the hospitality industry increase corporate value aligned with
   sustainable development goals? Case examples from Hilton, Melia and Sun},
Journal = {WORLDWIDE HOSPITALITY AND TOURISM THEMES},
Year = {2020},
