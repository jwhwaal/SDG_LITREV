@article{ WOS:000743520200010,
Author = {Weybrecht, Giselle},
Title = {Business schools are embracing the SDGs - But is it enough? - How
   business schools are reporting on their engagement in the SDGs},
Journal = {INTERNATIONAL JOURNAL OF MANAGEMENT EDUCATION},
Year = {2022},
Volume = {20},
Number = {1},
Month = {MAR},
