@article{ WOS:000754996800001,
Author = {Ordonez-Ponce, Eduardo and Weber, Olaf},
Title = {Multinational financial corporations and the sustainable development
   goals in developing countries},
Journal = {JOURNAL OF ENVIRONMENTAL PLANNING AND MANAGEMENT},
Year = {2022},
Volume = {65},
