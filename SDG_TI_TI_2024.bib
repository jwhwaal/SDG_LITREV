@article{ WOS:000808469800001,
Author = {Sinkovics, Noemi and Vieira, Luciana Marques and van Tulder, Rob},
Title = {Working toward the sustainable development goals in earnest - critical
   international business perspectives on designing and implementing better
   interventions},
Journal = {CRITICAL PERSPECTIVES ON INTERNATIONAL BUSINESS},
Year = {2022},
