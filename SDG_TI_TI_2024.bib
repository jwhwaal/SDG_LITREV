@article{ WOS:000735190700001,
Author = {Costa, Antonio and Tafuro, Alessandra and Benvenuto, Marco and Viola,
   Carmine},
Title = {Corporate Social Responsibility through SDGs: Preliminary Results from a
   Pilot Study in Italian Universities},
Journal = {ADMINISTRATIVE SCIENCES},
