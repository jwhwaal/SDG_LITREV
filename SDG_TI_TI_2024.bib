@article{ WOS:000868692800001,
Author = {Toukabri, Mohamed and Youssef, Mohamed Ahmed Mohamed},
Title = {Climate change disclosure and sustainable development goals (SDGs) of
   the 2030 agenda: the moderating role of corporate governance},
Journal = {JOURNAL OF INFORMATION COMMUNICATION \& ETHICS IN SOCIETY},
Year = {2023},
Volume = {21},
