Title = {Multinational corporations and sustainable development goals: an
   interventionist approach toward quality tertiary education in Nigeria's
   case study (Etisalat)},
Journal = {INNOVATION-THE EUROPEAN JOURNAL OF SOCIAL SCIENCE RESEARCH},
