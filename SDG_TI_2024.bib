@article{ WOS:000942084300001,
Author = {Shimamoto, Mihoko},
Title = {Normative Corporate Income Tax with Rent for SDGs' Funding: Case of the
   US},
Journal = {SUSTAINABILITY},
Year = {2023},
Volume = {15},
Number = {4},
Month = {FEB},
Abstract = {The purpose of this study is to explain the justification for taxing
   corporate rents as a funding source for Sustainable Development Goals
   (SDGs), and to calculate a normative corporate tax rate that takes into
   account rents for corporations, especially multinational corporations,
   and to recommend that the current corporate tax surcharge be used to
   finance social common capital. Considering global tax avoidance, we
   propose that many countries cooperate to raise their corporate taxes and
   finance SDGs. Aiming to calculate a normative corporate tax rate with
   rents for each country, we applied the total factor productivity method
   for calculating the markup rate, assumed long-term interest rates to be
   the marginal efficiency of capital, and developed a normative corporate
   tax rate calculation method. Using a Cobb-Douglas function in dynamic
   pseudo-competitive profit optimal conditions, we calculated the rents of
   234 American corporations listed on the S\&P 500 index. The normative
   tax rates from 1982 to 2014 for these companies are stable at 40 to
   60\%, whereas corporate income tax has gradually decreased from 40\% to
   less than 30\%. Thus, the amount lost due to the race to the bottom of
   corporate taxes can be used to finance the SDGs.},
