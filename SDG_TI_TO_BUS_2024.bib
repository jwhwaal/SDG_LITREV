@article{ WOS:000568741100001,
Author = {Pizzi, Simone and Rosati, Francesco and Venturelli, Andrea},
Title = {The determinants of business contribution to the 2030 Agenda:
   Introducing the SDG Reporting Score},
Journal = {BUSINESS STRATEGY AND THE ENVIRONMENT},
Year = {2021},
Volume = {30},
