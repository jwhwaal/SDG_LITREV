@article{ WOS:000617884200004,
Author = {Sebestova, Jarmila and Sroka, Wlodzimierz},
Title = {SUSTAINABLE DEVELOPMENT GOALS AND SME DECISIONS: THE CZECH REPUBLIC VS.
   POLAND},
