@article{ WOS:000648943700001,
Author = {Diaz-Sarachaga, Jose Manuel},
Title = {Monetizing impacts of Spanish companies toward the Sustainable
   Development Goals},
Journal = {CORPORATE SOCIAL RESPONSIBILITY AND ENVIRONMENTAL MANAGEMENT},
Year = {2021},
Volume = {28},
