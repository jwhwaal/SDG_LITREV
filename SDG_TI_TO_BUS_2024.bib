@article{ WOS:000867831100005,
Author = {Saxena, Anupam and Shanker, Sugandha},
Title = {Relevance of small businesses in protecting a wildlife sanctuary and
   meeting sustainable development goals},
Journal = {INTERNATIONAL JOURNAL OF INDIAN CULTURE AND BUSINESS MANAGEMENT},
Year = {2022},
