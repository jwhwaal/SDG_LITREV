@article{ WOS:000943060200001,
Author = {Dadsena, Krishna Kumar and Pant, Pushpesh},
Title = {Analyzing the barriers in supply chain digitization: sustainable
   development goals perspective},
Journal = {OPERATIONS MANAGEMENT RESEARCH},
Year = {2023},
Volume = {16},
