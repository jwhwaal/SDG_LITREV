@article{ WOS:000612221800001,
Author = {Esteves, Ana Margarida and Genus, Audley and Henfrey, Thomas and
   Penha-Lopes, Gil and East, May},
Title = {Sustainable entrepreneurship and the Sustainable Development Goals:
   Community-led initiatives, the social solidarity economy and commons
   ecologies},
