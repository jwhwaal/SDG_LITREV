@article{ WOS:000489328300004,
Author = {Christ, Katherine Leanne and Burritt, Roger Leonard},
Title = {Implementation of sustainable development goals: The role for business
   academics},
Journal = {AUSTRALIAN JOURNAL OF MANAGEMENT},
Year = {2019},
Volume = {44},
