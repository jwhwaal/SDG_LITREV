@article{ WOS:000671244500009,
Author = {van Zanten, Jan Anton and van Tulder, Rob},
Title = {Beyond COVID-19: Applying ``SDG logics{''} for resilient transformations},
Journal = {JOURNAL OF INTERNATIONAL BUSINESS POLICY},
Year = {2020},
Volume = {3},
