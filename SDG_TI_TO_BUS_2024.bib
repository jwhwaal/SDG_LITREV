@article{ WOS:000613770700001,
Author = {Lassala, Carlos and Orero-Blat, Maria and Ribeiro-Navarrete, Samuel},
Title = {The financial performance of listed companies in pursuit of the
   Sustainable Development Goals (SDG)},
Journal = {ECONOMIC RESEARCH-EKONOMSKA ISTRAZIVANJA},
Year = {2021},
Volume = {34},
